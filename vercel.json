{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
    {
      "src": "/includes/(.*)",
      "status": 403
    },
    {
      "src": "/db.sql",
      "status": 403
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*).php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    }
  ]
}